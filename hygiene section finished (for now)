          this._goodHabitCategoryElementClasses.hygeineElementClass
        );
        hygieneSelect.id = this._goodHabitCategoryIds.hygieneSelectId;
        hygieneContainer.appendChild(hygieneSelect);
        const hygieneSelectOption = document.createElement(
          this._goodHabitCategoryElements.optionElement
        );
        hygieneSelectOption.classList.add(
          this._goodHabitCategoryElementClasses.hygeineOptionClass
        );
        hygieneSelectOption.id = this._goodHabitCategoryIds.hygieneSelectId;
        hygieneSelectOption.value =
          this._goodHabitCategoryValues.hygieneSelectValue;
        hygieneSelectOption.textContent = "Select";
        hygieneSelect.appendChild(hygieneSelectOption);
        const hygieneOptionOne = document.createElement(
          this._goodHabitCategoryElements.optionElement
        );
        hygieneOptionOne.classList.add(
          this._goodHabitCategoryElementClasses.hygieneOptionClass
        );
        hygieneOptionOne.id = this._goodHabitCategoryIds.hygieneOptionOneId;
        hygieneOptionOne.value =
          this._goodHabitCategoryValues.firstHygieneValue;
        hygieneOptionOne.textContent = "Brush Teeth Atleast Once A Day";
        hygieneSelect.appendChild(hygieneOptionOne);
        const hygieneOptionTwo = document.createElement(
          this._goodHabitCategoryElements.optionElement
        );
        hygieneOptionTwo.classList.add(
          this._goodHabitCategoryElementClasses.hygieneOptionClass
        );
        hygieneOptionTwo.id = this._goodHabitCategoryIds.hygieneOptionTwoId;
        hygieneOptionTwo.value =
          this._goodHabitCategoryValues.secondHygieneValue;
        hygieneOptionTwo.textContent = "Shower Atleast Once a Day";
        hygieneSelect.appendChild(hygieneOptionTwo);
        const hygieneOptionThree = document.createElement(
          this._goodHabitCategoryElements.optionElement

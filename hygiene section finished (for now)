else if (setOption.value === "hygiene") {
        const hygieneContainer = document.createElement(
          this._goodHabitCategoryElements.containerElement
        );
        hygieneContainer.id =
          this._goodHabitCategoryContainerIds.hygieneContainerId;
        goodHabitForm.appendChild(hygieneContainer);
        const hygieneLabel = document.createElement(
          this._goodHabitCategoryElements.labelElement
        );
        hygieneLabel.classList.add(
          this._goodHabitCategoryElementClasses.hygieneElementClass
        );
        hygieneLabel.id = this._goodHabitCategoryIds.hygieneLabelId;
        hygieneLabel.textContent =
          "I saw you picked hygeine! Let's get more specific. What habit are you wanting to start?";
        hygieneContainer.appendChild(hygieneLabel);
        const hygieneSelect = document.createElement(
          this._goodHabitCategoryElements.selectElement
        );

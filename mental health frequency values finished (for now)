 const mentalHealthConditional =
          document.getElementById("mentalHealthSelect");
        console.log(mentalHealthConditional);
        mentalHealthConditional.addEventListener("change", () => {
          if (mentalHealthConditional.value === "practicingGratitude") {
            const gratitudeContainer = document.createElement(
              this._goodHabitCategoryElements.containerElement
            );
            gratitudeContainer.id =
              this._goodHabitCategoryContainerIds.mentalHealthGratitudeFrequencyContainerId;
            goodHabitForm.appendChild(gratitudeContainer);
            const gratitudeLabel = document.createElement(
              this._goodHabitCategoryElements.labelElement
            );
            gratitudeLabel.classList.add(
              this._goodHabitCategoryElementClasses
                .mentalHealthGratitudeElementClass
            );
            gratitudeLabel.id =
              this._goodHabitCategoryIds.mentalHealthFrequencyLabelId;
            gratitudeLabel.textContent =
              "You chose gratitude as a habit! What is or what will be your frequency of practicing gratitude?";
            gratitudeContainer.appendChild(gratitudeLabel);
            const gratitudeSelect = document.createElement(
              this._goodHabitCategoryElements.selectElement
            );
            gratitudeSelect.classList.add(
              this._goodHabitCategoryElementClasses
                .mentalHealthGratitudeElementClass
            );
            gratitudeSelect.id = this._goodHabitCategoryIds.gratitudeSelect;
            gratitudeContainer.appendChild(gratitudeSelect);
            const gratitudeSelectOption = document.createElement(
              this._goodHabitCategoryElements.optionElement
            );
